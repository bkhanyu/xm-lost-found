is.loadData(1)\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    loadData(pageNum) {\n      if (pageNum) this.pageNum = pageNum\n      this.$request.get('/message/selectPage', {\n        params: {\n          fromId: this.user.id,\n          pageNum: this.pageNum,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableData = res.data?.list\n          this.total = res.data?.total\n        }\n      })\n    },\n    del(id) {   // 单个删除\n      this.$confirm('您确定删除吗？', '确认删除', {type: \"warning\"}).then(response => {\n        this.$request.delete('/message/delete/' + id).then(res => {\n          if (res.code === '200') {   // 表示操作成功\n            this.$message.success('操作成功')\n            this.loadData(1)\n          } else {\n            this.$message.error(res.msg)  // 弹出错误的信息\n          }\n        })\n      }).catch(() => {\n      })\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum\n      this.loadData(this.pageNum)\n    }\n  }\n}\n</script>\n"],"mappings":"AA6CA,OAAAA,CAAA;AACA;EAEAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,SAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,QAAA;EACA;EACA;EACAC,OAAA;IACAD,SAAAJ,OAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAM,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAC,MAAA,OAAAf,IAAA,CAAAgB,EAAA;UACAV,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA;QACA;MACA,GAAAU,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAd,SAAA,GAAAa,GAAA,CAAAnB,IAAA,EAAAqB,IAAA;UACA,KAAAZ,KAAA,GAAAU,GAAA,CAAAnB,IAAA,EAAAS,KAAA;QACA;MACA;IACA;IACAa,IAAAL,EAAA;MAAA;MACA,KAAAM,QAAA;QAAAC,IAAA;MAAA,GAAAN,IAAA,CAAAO,QAAA;QACA,KAAAZ,QAAA,CAAAa,MAAA,sBAAAT,EAAA,EAAAC,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YAAA;YACA,KAAAO,QAAA,CAAAC,OAAA;YACA,KAAAjB,QAAA;UACA;YACA,KAAAgB,QAAA,CAAAE,KAAA,CAAAV,GAAA,CAAAW,GAAA;UACA;QACA;MACA,GAAAC,KAAA,QACA;IACA;IACAC,oBAAAzB,OAAA;MACA,KAAAA,OAAA,GAAAA,OAAA;MACA,KAAAI,QAAA,MAAAJ,OAAA;IACA;EACA;AACA","